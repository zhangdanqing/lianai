.banner{
    width:100%;
    display: block;
}
.title{
    font-size: 40rpx;
    height:150rpx;
    line-height: 150rpx;
    text-align: center;
    width:100%;
    display: block;
    text-align: center;
    color: #391635;
}
.block{
    height:26rpx;
    background: #ebebeb;
}
.content{
    padding: 0 40rpx;
}
.regist .section__title{
    font-size: 32rpx;
    color:#f95b5c;
    margin-bottom: 46rpx;
}
.section,.radio-group{
    padding:60rpx 0 30rpx;
    border-bottom:1px solid #e4e4e4;
}
picker,input,.placeholder,textarea,.radio{
    font-size: 32rpx;
    color:#6a6a6a;
}
.regist-btn{
    background:#f95b5c;
    color:#fff;
    margin-bottom:115rpx;
    margin-top: 100rpx;
}
.checkbox{
    margin-bottom:42rpx;
    margin-top: 62rpx;
}
checkbox{
    margin-right: 12rpx;
}
checkbox,.agree{
    float: left;
}
.checkbox view{
    font-size: 32rpx;
    color:#6a6a6a;
    padding-top: 2rpx;
}
.checkbox label{
    float: left;
}
.checkbox navigator{
    float: left;
    color:#334c83;
    text-decoration: underline;
}
.upload-photos{
    width:150rpx;
    height:150rpx;
    position: absolute;
    left:50%;
    top:50%;
    margin-left: -75rpx;
    margin-top:-75rpx;
}
.upload-image{
    width:200rpx;
    height:200rpx;
    margin-right: 20rpx;
}
.upload-image-box{
    width:200rpx;
    height:200rpx;
    border:1px solid #e5e5e5;
    background: #f7f6f2;
    display: inline-block;
    position: relative;
    box-sizing: border-box;
}
.change-image{
    display: block;
    padding-top: 20rpx;
    font-size: 34rpx;
    color:#111d78;
    text-decoration: underline;
}
