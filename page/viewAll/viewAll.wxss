.header{
    width:100%;
    background: #f95b5c;
    padding: 40rpx 60rpx;
}
.head-photo{
    width:200rpx;
    height:200rpx;
    float: left;
    margin-right: 50rpx;
}
.header-info{
    float: left;
}
.header-infor{
    padding-top: 30rpx;
    color: #fff;
}
.header-infor-title{
    font-size: 38rpx;
    margin-bottom: 20rpx;
    display: inline-block;
}
.infor-list text{
    padding:0 38rpx;
    border-left: 2px solid #fff;
    font-size: 32rpx;
    height:32rpx;
    display: inline-block;
    line-height: 32rpx;
}
.infor-content{
    background: #f2f6f9;
    padding: 30rpx 35rpx;
}
.basic-infor{
    background: #fff;
}
.infor-title{
    font-size: 36rpx;
    height:36rpx;
    line-height: 36rpx;
    padding-left: 36rpx;
    border-left: 2px solid #f95b5c;
    color:#595959;
}
.infor-title-box{
    width:100%;
    padding: 45rpx 0 30rpx 20rpx;
}
.infor-body{
    border-top: 1px solid #f4f4f4;
    padding: 50rpx 50rpx;
    font-size:32rpx;
    color:#595959;
}
.basic-body text{
    padding: 13rpx 40rpx;
    background: #f2f6f9;
    border-radius: 30rpx;
    float: left;
    margin-right:30rpx;
    color:#525252;
}
.basic-tag{
    margin-bottom: 30rpx;
}
