.title{
    font-size: 40rpx;
    height:160rpx;
    line-height: 160rpx;
    text-align: center;
    width:100%;
    display: block;
    text-align: center;
}
.block{
    height:26rpx;
    background: #ebebeb;
}
.content{
    padding: 0 40rpx;
}
.regist .section__title{
    font-size: 32rpx;
    color:#f95b5c;
    margin-bottom: 46rpx;
}
.section{
    padding:60rpx 0 30rpx;
    border-bottom:1px solid #e4e4e4;
}
picker,input,.placeholder,.textarea{
    font-size: 32rpx;
    color:#6a6a6a;
}
.regist-btn{
    background:#f95b5c;
    color:#fff;
    margin-bottom:115rpx;
}
.checkbox{
    margin-bottom:42rpx;
    margin-top: 62rpx;
}
checkbox{
    margin-right: 12rpx;
}
checkbox,.agree{
    float: left;
}
.checkbox view{
    font-size: 32rpx;
    color:#6a6a6a;
    display: inline-block;
    padding-top: 2rpx;
}
.checkbox view navigator{
    display: inline-block;
    color:#334c83;
}
