/* reset */
page {
  background-color: #F8F8F8;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}
input,view,picker,text{
    padding: 0;
    margin:0;
}
.clearfix:after{content:"";display:block;height:0;clear:both;visibility:hidden}
.clearfix{zoom: 1;}
/*搜索框*/
.home-box{
    /*width:100%;*/
    background: #f5f5f5;
    padding-top:30rpx;
    padding-bottom: 30rpx;
    margin-bottom: 30rpx;
}
.search-box{
    height:70rpx;
    display: table;
    margin:0 auto;
    border-radius: 30rpx;
    width:85%;
    padding-left:30rpx;
    background: #fff;
}
.search-box icon{
    display: table-cell;
    vertical-align: middle;
    width:12rpx;
    padding-right: 12rpx;
}
.search-box input{
    font-size: 28rpx;
    height:100%;
    line-height: 70rpx;
}
.search-place{
    color: #7d7d7d;
}
